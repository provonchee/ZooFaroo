DROP TABLE IF EXISTS `barter_cities`;

CREATE TABLE `barter_cities` (
  `city_id` int(5) NOT NULL auto_increment,
  `state_id` int(2) default NULL,
  `city` varchar(50) default '',
  `coords` varchar(16) default '',
